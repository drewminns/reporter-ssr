{
  "name": "reporterv2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "fix:other": "npm run prettier --write",
    "fix:js": "npm run test:js --fix",
    "fix": "npm run fix:js && npm run fix:other",
    "prettier": "prettier \"**/*.{md,scss,yaml,yml,css}\"",
    "test:js": "node_modules/.bin/eslint --ext .js .",
    "start": "npm run build && npm run server:run",
    "dev": "run-p watch server",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "server": "nodemon server.js --watch server.js --watch api --watch public/App.js",
    "server:run": "node server.js"
  },
  "browsers": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "node_modules/.bin/eslint --fix",
      "git add"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "express": "^4.17.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.0.0",
    "sirv-cli": "^0.4.2",
    "svelte": "^3.4.2",
    "svelte-preprocess": "^2.9.0",
    "svelte-routing": "^1.1.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-svelte3": "^1.2.3",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-notify": "^1.1.0",
    "rollup-plugin-sizes": "^0.5.1"
  }
}
